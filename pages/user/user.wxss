/* 自定义当前页的头部背景色 */

.bg-color {
  background: #fff;
}

.cus-header {
  left: 0;
  right: 0;
  position: fixed;
  z-index: 999;
  background: linear-gradient(90deg, rgba(255, 103, 96, 1), rgba(255, 147, 118, 1));
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

page {
  position: relative;
}

.header {
  position: relative;
  width: 100%;
  padding-bottom: 134rpx;
  color: #fff;
  font-size: 26rpx;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-position: center;
  background-size: 100% 100%;
}

.user-wrapper {
  padding-bottom: 68rpx;
  margin: 39rpx 20rpx 0 14rpx;
  position: relative;
  display: flex;
}

.user-wrapper .avatar-wrap {
  position: relative;
}

.user-wrapper .avatar-wrap .identify {
  position: absolute;
  bottom: -18rpx;
  border: 2px solid #fff;
  padding: 0rpx 12rpx;
  height: 38rpx;
  line-height: 38rpx;
  border-radius: 19rpx;
  background: #ff6f6c;
  display: flex;
  align-items: center;
  left: 50%;
  transform: translateX(-50%);
  white-space: nowrap;
  overflow: hidden;
}

.user-wrapper .avatar-wrap .identify image {
  width: 23rpx;
  height: 23rpx;
  margin-right: 8rpx;
}

.user-wrapper .avatar-wrap .identify .txt {
  font-size: 20rpx;
  color: #fff;
  max-width: 100rpx;
  overflow: hidden;
}

.my-scores-wrap {
  position: absolute;
  height: 54rpx;
  border-radius: 27rpx;
  background-color: #fde9b1;
  display: flex;
  align-items: center;
  color: #333;
  font-size: 24rpx;
  padding: 0 10rpx;
  left: 158rpx;
  bottom: 43rpx;
}

.my-scores-wrap .scores {
  padding: 0 5rpx;
}

.header .system-notification {
  position: absolute;
  right: 30rpx;
  top: 46rpx;
  font-size: 46rpx;
}

.icon-scores, .icon-settled {
  color: #f8b303;
  font-size: 30rpx !important;
}

.icon-book, .icon-order, .icon-star, .icon-coupon, .icon-share, .icon-apprentice,
.icon-rentou1, .icon-service, .icon-settings, .icon-exit {
  color: #f8b303;
  font-size: 32rpx !important;
}

.settled {
  padding: 0 5rpx;
  font-size: 24rpx !important;
  color: #fff;
}

.header .system-notification .system-notification-text {
  position: absolute;
  top: -5rpx;
  right: -3rpx;
  font-size: 18rpx;
  background-color: #fff;
  border-radius: 50%;
  color: #ff3d3d;
  min-width: 26rpx;
  height: 26rpx;
  text-align: center;
  line-height: 26rpx;
}

.header .avatar {
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
  border: 6rpx solid rgba(255, 255, 255, 0.5);
  float: left;
}

.header .name {
  margin: 10rpx 0 7rpx 16rpx;
  flex: 1;
  position: relative;
  padding-right: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.header .name .nick {
  margin-top: 15rpx;
}

.header .name .follow-fans {
  display: flex;
}

.header .name .follow-fans .item {
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
}

.header .name .follow-fans .follows {
  margin-right: 15rpx;
}

.nick {
  font-size: 32rpx;
}

.header .name>view {
  margin-bottom: 10rpx;
}

/* 我的资料 */

.profile-wrapper {
  position: absolute;
  right: 0;
  bottom: 182rpx;
  height: 74rpx;
  line-height: 74rpx;
  border-radius: 37rpx 0 0 37rpx;
  padding: 0 30rpx;
  z-index: 99;
  background: linear-gradient(90deg, #fc5866, #f95799);
}

.profile-wrapper.identify {
  bottom: 274rpx;
  background: #ffad9f;
}

.icon-search {
  margin-right: 5rpx;
}

.follow {
  top: 134rpx;
  background-image: -webkit-gradient(linear, 79% 0%, 0% 100%, from(#29323b), to(#59636d));
}

.icon-xingxing1, .icon-icon- {
  font-size: 32rpx !important;
  margin-right: 6rpx;
}

/* 我的订单 */

.item-box {
  position: absolute;
  left: 14rpx;
  right: 14rpx;
  margin-bottom: 26rpx;
  background-color: #fff;
  border-radius: 5px;
  box-shadow: 1px 2px 2px 1px #eee;
  bottom: -110rpx;
}

.amount {
  height: 150rpx;
  align-items: center;
}

.amount .amount-item {
  flex: 1 25%;
  text-align: center;
  font-size: 24rpx;
  color: #555;
}

.amount .amount-item .count {
  margin-bottom: 17rpx;
  font-size: 32rpx;
  color: #ff3d3d;
}

.title-bar {
  padding: 0 28rpx;
  height: 88rpx;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
}

.title-bar navigator {
  font-size: 22rpx;
  color: #999;
}

.list-wrapper {
  flex-wrap: wrap;
  align-items: center;
}

.list-wrapper .item {
  width: 20%;
  text-align: center;
  margin: 20rpx 0;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.list-wrapper .item navigator {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.list-wrapper .item .iconfont {
  font-size: 45rpx;
}

.list-wrapper .item .text {
  font-size: 24rpx;
  color: #000;
  margin-top: 10rpx;
}

.orders image {
  width: 52rpx;
  height: 52rpx;
}

.orders .list-wrapper {
  height: 142rpx;
}

.construction-order {
  background-color: #fff;
  margin-top: 120rpx;
}

.tabs-wrap {
  height: 80rpx;
  box-sizing: border-box;
}

.tabs-wrap .tab-item {
  display: flex;
  justify-content: center;
  align-items: center;
  flex: 1;
  border-bottom: 1px solid #ccc;
}

.tabs-wrap .active {
  color: #f8b303;
  border-bottom: 1px solid #f8b303;
}

.content-wrap {
  min-height: 600rpx;
}

.item-wraper {
  display: flex;
  padding: 20rpx 25rpx;
  border-bottom: 1px solid #ededed;
  border-top: 1px solid #ededed;
  margin-bottom: 30rpx;
}

.content-item .img-wrap {
  flex: 0 0 150rpx;
  height: 150rpx;
  position: relative;
}

.content-item .img-wrap .img {
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}

.content-item .img-wrap .rank {
  width: 40rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  left: 10rpx;
}

.content-item .detail {
  padding: 10rpx 0 10rpx 20rpx;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  position: relative;
  overflow: hidden;
}

.content-item .detail .btn {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  width: 120rpx;
  padding: 15rpx 0;
  background: -webkit-gradient(linear, 79% 0%, 0% 100%, from(#5f6973), to(#a4aeb9));
  color: #fff;
  font-size: 26rpx;
  border-radius: 30rpx;
  line-height: 26rpx;
  text-align: center;
}

.red-like {
  color: #fc4953;
}

.content-item .detail .name {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  font-size: 30rpx;
}

.content-item .detail .desc {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 24rpx;
  color: #f8b303;
}

.bundle {
  width: 2px;
  height: 20rpx;
  background-color: #f8b303;
  margin-right: 15rpx;
}

.order-number {
  padding: 0 25rpx;
  overflow: hidden;
  font-size: 24rpx;
  color: #666;
}

.order-number>view {
  height: 70rpx;
  line-height: 70rpx;
}

.order-number .num {
  float: left;
}

.order-number .time {
  float: right;
}

.nomore {
  padding: 20rpx;
  text-align: center;
  font-size: 24rpx;
  color: #5f6973;
}

.balance {
  margin: 0 14rpx;
  height: 122rpx;
  background: #fff;
  position: relative;
  top: 100rpx;
  padding: 0 49rpx 0 42rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 10rpx;
}

.balance .btn {
  background: #ff605a;
  color: #fff;
  width: 110rpx;
  height: 54rpx;
  line-height: 54rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 10rpx;
}

.signal {
  font-size: 22rpx;
}

.num-b {
  font-size: 40rpx;
}

.appoint-wrapper {
  padding: 30rpx 24rpx;
  font-size: 30rpx;
  color: #666;
  background: #fff;
}

.name-col {
  display: flex;
}

.bottom {
  border-bottom: 1px solid #eee;
}

.vip-wrap {
  margin-left: 15rpx;
  width: 152rpx;
  height: 47rpx;
}

.all-wrap {
  margin: 0 14rpx 0rpx;
  border-radius: 10rpx;
  margin-top: 16rpx;
  background: #fff;
  padding: 16rpx 0;
}
