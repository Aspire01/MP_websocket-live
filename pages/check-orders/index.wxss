/* pages/check-orders/index.wxss */

.order-sn > image {
  width: 27rpx;
  height: 30rpx;
}

.menu {
  background-color: #fff;
  border-bottom: 1px solid #f7f7f7;
}

.menu .item {
  width: 25%;
  height: 85rpx;
  line-height: 85rpx;
  text-align: center;
  box-sizing: border-box;
}

.menu .item.on {
  color: #ff605a;
  border-bottom: 1px solid #ff605a;
}

.wrapper {
  margin: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  position: relative;
}

.goods-info-wrap {
  border-bottom: 1px solid #eee;
}

.order-sn {
  padding: 20rpx;
  font-size: 26rpx;
  color: #666;
}

.order-list-wrapper {
  background-color: #fff;
  border-radius: 20rpx;
}

.order-item  .top {
  padding: 20rpx;
}

.name {
  color: #666;
  margin-right: 10rpx;
}

.combo {
  height: 30rpx;
  font-size: 20rpx;
  color: #fff;
  border-radius: 15rpx;
  padding: 10rpx;
  background: #ff605a;
  border-bottom: 1px solid #eee;
}

.content-wrap {
  min-height: 600rpx;
}

.content-item {
  display: flex;
  padding: 20rpx 25rpx;
}

.content-item .img-wrap {
  flex: 0 0 170rpx;
  height: 170rpx;
  position: relative;
}

.content-item .img-wrap .img {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.content-item .img-wrap .rank {
  width: 40rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  left: 10rpx;
}

.content-item .detail {
  padding: 20rpx 0 20rpx 20rpx;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  position: relative;
}

.red-like {
  color: #222B34;
  font-weight: 500;
}

.content-item .detail .name {
  width: 320rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  font-size: 30rpx;
}

.content-item .detail .desc {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 24rpx;
  color: #707070;
  line-height: 1.75;
}

.hj {
  padding: 10rpx 20rpx;
  text-align: right;
  font-size: 24rpx;
}

.btn-wrap {
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
}

.btn {
  height: 42rpx;
  border-radius: 21rpx;
  padding: 8rpx 20rpx;
  color: #fff;
  box-sizing: border-box;
  border: 1px solid #ff605a;
  margin-left: 20rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bg {
    background: linear-gradient(90deg, rgba(255, 103, 96, 1), rgba(255, 147, 118, 1));
}

.again {
  width: 160rpx;
  margin: 10rpx 0;
  border: 1px solid #ccc;
  color: #000;
}

.again-wrap {
  justify-content: flex-end;
  align-items: center;
  padding: 0 20rpx;
}

.hasMore {
  text-align: center;
  padding: 10rpx;
  color: #ccc;
  font-size: 28rpx;
}

.empty-order-wrapper {
  margin: 40rpx;
}

.img-show-wrap {
  margin: 0 auto;
  width: 300rpx;
  height: 196rpx;
  /* background: red; */
  background-image:url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100%;
  background-position: center;
}

.empty-text-wrap {
  margin-top: 28rpx;
  font-size: 24rpx;
  color: #AEACAC;
  text-align: center;
}

.icon-delete {
  color: #ADADAD;
}

.group-order-type-wrap {
  position: absolute;
  right:-6rpx;
  top: 10rpx;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  font-size: 24rpx;
  background: #ff605a;
  color: #fff;
}