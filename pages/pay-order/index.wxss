page, .container {
  width: 100%;
  background: #f5f5f5;
  margin-bottom: 100rpx;
}

.address-info {
  background: #fff;
}

.address-content {
  display: flex;
  flex-wrap: wrap;
  padding: 35rpx 25rpx;
  min-height: 158rpx;
  box-sizing: border-box;
}

.localtion-icon {
  width: 27rpx;
  height: 33rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
  margin-right: 25rpx;
}

.address-detail {
  flex: 1;
  line-height: 30rpx;
  color: #666;
  overflow: hidden;
}

.user-msg {
  overflow: hidden;
}

.address-detail .user {
  float: left;
  width: 65%;
}

.address-detail .phone {
  float: right;
}

.address-detail .address {
  line-height: 40rpx;
  margin-top: 10rpx;
  position: relative;
}

.address-detail .address text {
  display: inline-block;
  width: 90%;
}

.address-detail .address .edit-icon {
  width: 28rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: cover;
  position: absolute;
  right: 0;
  top: 5rpx;
}

.pay {
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 15rpx;
}

.pay .title {
  font-size: 28rpx;
  color: #999;
}

.pay .payway-list .payway-item {
  display: flex;
  margin-top: 25rpx;
  margin-bottom: 5rpx;
}

.pay .payway-list .payway-item .payway-icon,
.pay .payway-list .payway-item .payway-icon image {
  width: 64rpx;
  height: 64rpx;
}

.pay .payway-list .payway-item .payway-name {
  color: #333;
  line-height: 30rpx;
  flex: 1;
  margin-left: 25rpx;
}

.payway-desc {
  font-size: 24rpx;
  color: #999;
  margin-top: 5rpx;
}

.pay .payway-list .payway-item .select image {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

.pay .payway-list .payway-item .select {
  position: relative;
  width: 32rpx;
}

.order {
  background: #fff;
  margin-top: 15rpx;
  padding: 20rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.order .order-item {
  display: flex;
  margin: 15rpx 0;
  position: relative;
  align-items: center;
  min-height: 121rpx;
}

.order .order-item .order-icon, .order .order-item .order-icon image {
  width: 162rpx;
  height: 162rpx;
}

.order .order-item .order-content {
  flex: 1;
  line-height: 40rpx;
  padding: 14rpx 0;
  box-sizing: border-box;
  margin-left: 15rpx;
}

.order .order-item .order-content .style {
  margin: 7rpx 0;
  font-size: 24rpx;
  color: #999;
}

.order .order-item .order-content .price {
  color: #E25D58;
  font-size: 36rpx;
}

.order .order-item .order-content .order-title .goods_name {
  width: 400rpx;
}

.btn {
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  color: #999;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}

.btn view {
  float: left;
}

.btn .reduce, .btn .add {
  height: 32rpx;
  border: 1rpx solid #999;
  box-sizing: border-box;
  width: 32rpx;
  line-height: 32rpx;
}

.btn .num {
  padding: 0 15rpx;
}

.liuyan {
  overflow: hidden;
  width: 100%;
}

.liuyan-input {
  width: 100%;
  height: 140rpx;
}

.total {
  background: #fff;
  overflow: hidden;
  padding: 20rpx;
}

.total view {
  float: right;
}

.total text {
  margin-left: 10rpx;
  color: #E25D58;
}

.sub-order {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  z-index: 999;
}

.sub-order .pay-price {
  flex: 1;
  background: #fff;
}

.sub-order .sub-btn {
  width: 200rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #FF706A;
}

.sub-order .pay-price .txt {
  float: right;
  margin-right: 20rpx;
}


.els {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.action-list {
  width: 100%;
  height: 500rpx;
  box-sizing: border-box;
  overflow-y: auto;
  padding-bottom: 120rpx;
}

.action-list .item {
  width: 100%;
  height: 152rpx;
  box-sizing: border-box;
  padding: 39rpx 48rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.action-list .item .icon-select {
  width: 32rpx;
  height: 32rpx;
}

.action-list .item .desc {
  width: 442rpx;
  margin-left: 20rpx;
}

.action-list .item .desc .caption {
  font-size: 30rpx;
  color: #666;
  margin-bottom: 15rpx;
}

.action-list .item .desc .info {
  font-size: 28rpx;
  color: #666;
}

.action-list .item .desc .info .phone {
  margin-left: 40rpx;
}

.action-list .item .left {
  display: flex;
  align-items: center;
  text-align: left;
}

.action-list .item .iconfont {
  font-size: 46rpx;
}

.add-address {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 1rpx solid #eaeaea;
  background: #fff;
}

.add-address .iconfont {
  font-size: 45rpx;
  color: #FFC6BC;
}

.add-address .info {
  margin-left: 20rpx;
  font-size: 32rpx;
  color: #666;
}

.cu-modal .reserve {
  width: 170rpx;
  height: 60rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 60rpx;
  border-radius: 29rpx;
  background: #fbb637;
  margin: 25rpx auto 35rpx;
}

.modal-box {
  padding-top: 0;
}

/* 模态框样式color-ui 定金 */

.cu-modal .title {
  font-size: 24rpx;
  color: #333;
  margin-bottom: 60rpx;
}

.cu-modal .price {
  font-size: 46rpx;
  color: #e2631c;
}

/* 优惠券 */

.available-coupon-wrapper {
  padding: 15rpx 0;
  margin-top: 15rpx;
}

/* 优惠券ends */

/* 优惠券model */

.coupon-list-wrap {
  margin-top: 10rpx;
  min-height: 450rpx;
}

.coupon-item {
  height: 205rpx;
  background: #fff;
  display: flex;
  border-radius: 10rpx;
  position: relative;
}

.coupon-item .left {
  flex: 0 0 205rpx;
  background: #f98c58;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.coupon-item .left .num {
  margin-top: 60rpx;
  margin-bottom: 20rpx;
  color: #fff;
  font-family: 'Impact';
}

.coupon-item .left .symbol {
  font-size: 24rpx;
}

.coupon-item .left .big {
  font-size: 50rpx;
}

.coupon-item .left .good {
  height: 32rpx;
  width: 98rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 16rpx;
  background: #fff;
  color: #f98c58;
  font-size: 20rpx;
}

.coupon-item .right {
  flex: 1;
  padding: 30rpx 18rpx 28rpx 36rpx;
  position: relative;
}

.coupon-item .right .valid {
  font-size: 24rpx;
  color: #999;
  margin-top: 21rpx;
}

.coupon-item .right .name {
  font-size: 36rpx;
  margin-bottom: 23rpx;
}

.coupon-item .right .discount {
  display: flex;
  align-items: center;
}

.coupon-item .right .discount .inner-discount {
  height: 36rpx;
  line-height: 36rpx;
  border-radius: 18rpx;
  color: #e2631c;
  letter-spacing: 4rpx;
  border: 1px solid #e2631c;
  font-size: 20rpx;
  padding: 0 20rpx;
  display: inline-block;
  max-width: 260rpx;
}

.get-status {
  height: 52rpx;
  border-radius: 26rpx;
  line-height: 52rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  width: 112rpx;
  position: absolute;
  right: 24rpx;
  top: 88rpx;
  background: #c3c3c3;
}

.half-circle {
  width: 40rpx;
  height: 20rpx;
  border-radius: 0 0 40rpx 40rpx;
  background: #fff;
  position: absolute;
  left: 186rpx;
  top: 14rpx;
  border: 1px solid #f2f2f2;
  border-top: 1px solid #f7f7f7;
  background: #f7f7f7;
}

.outer {
  padding: 15rpx 24rpx;
  position: relative;
}

.half-bot {
  width: 40rpx;
  height: 20rpx;
  border-radius: 40rpx 40rpx 0 0;
  background: #fff;
  position: absolute;
  left: 186rpx;
  bottom: 14rpx;
  border: 1px solid #f2f2f2;
  border-bottom: 1px solid #f7f7f7;
  background: #f7f7f7;
}

.un-get-status {
  height: 52rpx;
  border-radius: 26rpx;
  line-height: 52rpx;
  text-align: center;
  color: #fff;
  font-size: 24rpx;
  /* padding: 0 20rpx; */
  width: 112rpx;
  position: absolute;
  right: 24rpx;
  top: 88rpx;
  background: #f98c58;
}

/* 优惠券model ends */
