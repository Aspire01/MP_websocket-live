<cu-custom bgColor="common-base-bg" isBack="{{true}}">
  <view slot="content" class="common-title-color">联系客服</view>
</cu-custom>

<view class="wrapper">
  <view class='qr-wrap'>
    <image src='{{serviceInfo.qrcode}}'></image>
  </view>
  <view class='row-item'>微信搜索公众号【{{serviceInfo.name}}】或扫描上</view>
  <view class='row-item'>方二维码，关注公众号【{{serviceInfo.name}}】</view>
  <view class='row-item'>联系客服微信：{{serviceInfo.wx_number}}</view>
  <view class='row-item'>联系客服QQ:{{serviceInfo.qq}}</view>
</view>