page {
  background: #f3f3f3;
}

.confirm-wrapper {
  padding: 80rpx 65rpx;
}

.choose {
  color: #a9a8a8;
  font-size: 30rpx;
  margin-bottom: 40rpx;
  padding: 0 15rpx;
}

.choose-box {
  height: 74rpx;
  border-radius: 37rpx;
  background: #fff;
  border: 1px solid #eee;
  margin-bottom: 37rpx;
  position: relative;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #787878;
}

.icon-triangle-down {
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.identity-wrap {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding: 0 10rpx;
}

.identity-wrap .item {
  margin-right: 20rpx;
  padding: 10rpx 24rpx;
  background: #ccc;
  color: #fff;
  font-size: 24rpx;
}

.identity-wrap .active {
  background: #363636!important;
}

.draw-down-box {
  position: absolute;
  left: 0;
  top: 77rpx;
  width: 100%;
  z-index: 999;
  background: #f3f3f3;
  height: 300rpx;
}

.draw-down-box .draw-item {
  position: relative;
  height: 80rpx;
  width: 100%;
  line-height: 80rpx;
  padding: 0 20rpx;
  border-bottom: 1px solid #ddd;
}

.visible {
  display: block;
}

.none {
  display: none;
}

.work-type-wrapper {
  padding: 20rpx;

}

.scroll-view-H-type {
  width: 100%;
  white-space: nowrap;
}

.scroll-view-H-type .scroll-item {
  display: inline-block;
  padding: 10rpx;
  background: #ccc;
  font-size: 26rpx;
  margin-right: 20rpx;
}
