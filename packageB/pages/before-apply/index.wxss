page {
  background: #fefcee;
  height: 100%;
}

.img {
  position: absolute;
}

.img.img1 {
  width: 221rpx;
  height: 188rpx;
  left: 0;
}

.img.img2 {
  width: 127rpx;
  height: 141rpx;
  right: 0;
}

.img.img3 {
  width: 170rpx;
  height: 205rpx;
  right: 0;
  bottom: 0;
}

.img.img4 {
  width: 420rpx;
  height: 344rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -60%);
}

.nav {
  position: absolute;
  width: 478rpx;
  height: 64rpx;
  left: 50%;
  transform: translateX(-50%);
}

.nav1 {
  top: 67%;
}

.nav2 {
  top: 78%;
}

.btn {
  position: absolute;
  width: 100%;
  height: 100%;
  line-height: 64rpx;
  border-radius: 32rpx;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
}

.btn.platform {
  background: #f8b303;
}

.btn.agency {
  background: #ff9f5a;
}
